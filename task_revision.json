{
    "family": "hello_service",
    "containerDefinitions":
    [
      {
        "name": "hello-service",
        "image": "601458892984.dkr.ecr.us-west-2.amazonaws.com/hello-repository:latest",
        "cpu": 0,
        "memory": 128,
        "essential": true,
        "environment": [
          {
              "name": "REDIS_CLUSTER_ADDRESS",
              "value": "hello-cache.qomrhk.0001.usw2.cache.amazonaws.com:6379"
          },
          {
              "name": "ELB_NAME",
              "value": "hello-service-elb"
          }
        ],
        "portMappings": [
          {
            "containerPort": 80,
            "hostPort": 80
          }
        ]
      }
    ]

}